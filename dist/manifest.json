{
  "manifest_version": 3,
  "name": "Scrapper",
  "version": "1.0.0",
  "background": {
    "service_worker": "sw.js"
  },
  "action": {
    "deffault_title": "Click Me",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab",
    "downloads"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}